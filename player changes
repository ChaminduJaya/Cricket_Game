 def player_change(self):
        print(self.batsman)
        if self.batsman== self.player_a :

            self.batsman = self.player_b
            self.player_B_star = '*'
            self.player_A_star = ' '
        else :
            self.batsman = self.player_a
            self.player_A_star = '*'
            self.player_B_star = ' '
